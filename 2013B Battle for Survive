#include<bits/stdc++.h>
using namespace std;
 
int main(){
    long long int t;
    cin>>t;
    while(t--){
       long long int n;
        cin>>n;
        vector<long long int>v(n);
        long long int sum=0;
        for(long long int i=0;i<n;i++){
            cin>>v[i];
            sum+=v[i];
        }
        cout<<sum-2*v[n-2]<<endl;
    }
}
