#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        //int a[n][n];
        char ch;
        int flag=0;
        for(int i=0;i<n;i++){
            int c=0;
            for(int j=0;j<n;j++){
                cin>>ch;
                if(ch=='1')c++;
               
            }
             if(c==1)flag=1;
        }
        if(flag==1)cout<<"TRIANGLE"<<endl;
        else cout<<"SQUARE"<<endl;
    }
    return 0;
}
