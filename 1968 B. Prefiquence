#include <iostream>
using namespace std;

int main() {
   int t;
   cin>>t;
   while(t--){
       int n,m,count=0,c=0;
       string a,b;
       cin>>n>>m;
       cin>>a>>b;
       int j=0;
       for(int i=0;i<m;i++){
             if(b[i]==a[j]){
                 count++;
                 j++;
             }
             if(j==n)break;
           }
           cout<<count<<endl;
       }
       
       
   

    return 0;
}
