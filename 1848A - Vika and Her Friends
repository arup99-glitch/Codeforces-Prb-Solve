#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n,m,k;
        cin>>n>>m>>k;
        int x,y;
        cin>>x>>y;
        bool flag = true;
        for(int i=0;i<k;i++){
            int a,b;
            cin>>a>>b;
            if((a+b)%2==(x+y)%2){
                flag = false;
            }
        }
        if(flag){
            cout<<"YES"<<endl;
        }else{
            cout<<"NO"<<endl;
        }
     }

    return 0;
}
