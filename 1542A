#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int t;
    cin>>t;
    while(t--){
       int n,m=0;
       cin>>n;
       for(int i=0,x;i<n*2;i++){
           cin>>x;
           if(x%2!=0)m++;
       }
           if(m==n)cout<<"Yes"<<endl;
           else cout<<"No"<<endl;
           
       
    }
   
 
    return 0;
}
