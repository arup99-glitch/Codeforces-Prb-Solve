#include <bits/stdc++.h>
using namespace std;
int main() {
    
    int t;
    cin>>t;
    while(t--){
        string n,k;
        cin>>n>>k;
        bool count=false;
        for(int i=0,j=0;i<n.length();i++){
            if(n[i]==k[j] && i%2==0)count=true;
        }
        if(count)cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
        
    }

    return 0;
}
