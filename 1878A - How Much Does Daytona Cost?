#include <iostream>
using namespace std;
int main() {
    
    int t;
    cin>>t;
    while(t--){
        int n,k;
        cin>>n>>k;
        int count=0;
        for(int i=0;i<n;i++){
            int a;
            cin>>a;
            if(a==k)count++;
            
        }
        if(count)cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }

    return 0;
}
