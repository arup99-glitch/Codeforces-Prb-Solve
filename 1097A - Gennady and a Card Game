
#include <bits/stdc++.h>
using namespace std;

int main() {
    string a;
    cin>>a;
    string s;
    for(int i=0;i<5;i++){
        cin>>s;
        if(s[0]==a[0] || s[1]==a[1]){
        cout<<"YES"<<endl;
        return 0;
        }
    }
    cout<<"NO"<<endl;
    return 0;
    
   

    return 0;
}
