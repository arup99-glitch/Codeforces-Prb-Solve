#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n,count=0;
        cin>>n;
        for(int i=0;i<n;i++){
            int x;
            cin>>x;
            if(x%2 != 0)count++;
        }
        if(count%2==0)cout<<"Yes"<<endl;
        else cout<<"No"<<endl;
    }
    

    return 0;
}
