#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
       long long int a,b;
       cin>>a>>b;
       if(a%2==0)cout<<"YES"<<endl;
       else{
       long long int s = abs(a-b);
       if(s%2==0)cout<<"YES"<<endl;
       else cout<<"NO"<<endl;
       }
       
        
    }
    return 0;
}
