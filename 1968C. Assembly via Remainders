#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int t;
    cin>>t;
    while(t--){
      int n;
      cin>>n;
      vector<int>vec(n);
      for(int i=0;i<n-1;i++)cin>>vec[i];
      long long f=1000;
      cout<<1000<<" ";
      for(long long int i=0;i<n-1;i++){
          cout<<f+vec[i]<<" ";
          f+=vec[i];
      }
      cout<<endl;
      
    }
    return 0;
}
