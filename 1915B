#include<bits/stdc++.h>
using namespace std;

int main(){
int a,b,c,t;
char p[3][3],q;
cin>>t;
while(t--){
    a=0,b=0,c=0;
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cin>>p[i][j];
            if(p[i][j]=='A')a++;
            if(p[i][j]=='B')b++;
            if(p[i][j]=='C')c++;
        }
    }
    if(a!=3)q='A';
    if(b!=3)q='B';
    if(c!=3)q='C';
    cout<<q<<'\n';
}
}
