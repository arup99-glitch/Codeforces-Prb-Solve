#include<bits/stdc++.h>
using namespace std;
int main()
{
    double n,m,b;
    cin>>n;
    while(n--){
            double sum=0;
        cin>>m;
        for(int i=0;i<m;i++)
        {
            cin>>b;
            sum+=b;
        }
        long long sr = sqrt(sum);
        if(sr*sr==sum)cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }
}
