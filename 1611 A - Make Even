#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a=-1;
        if(n%2==0){
            a=0;
        }
        else{
            n=n/10;
            while(n>0){
                if(n%2==0){
                    if(n<10)a=1;
                    else a=2;
                }
                 n=n/2;
            }
           
        }
        cout<<a<<endl;
    }
    return 0;
}
