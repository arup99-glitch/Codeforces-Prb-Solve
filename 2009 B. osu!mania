#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int t;
    cin>>t;
    while(t--){
        int a;
        cin>>a;
        vector<int>v;
        while(a--){
             string s;
            cin>>s;
            for(int i=0;i<4;i++){
                if(s[i]=='#'){
                    v.push_back(i);
                }
                else continue;
            }
        }
            reverse(v.begin(),v.end());
            for(int i=0;i<v.size();i++){
            cout<<v[i]+1<<" ";
 
        }
        cout<<endl;    
    }
    return 0;
}
