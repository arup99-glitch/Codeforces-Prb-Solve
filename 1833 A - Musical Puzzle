#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string a;
        cin>>a;
        set<string>s;
        for(int i=0;i<n-1;i++){
            s.insert(a.substr(i,2));
        }
        cout<<s.size()<<endl;
    }
}
