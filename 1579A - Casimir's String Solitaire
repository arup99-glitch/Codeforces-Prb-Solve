#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
     string s;
     cin>>s;
     long a=0,b=0,c=0;
     for(int i=0;i<s.size();i++){
         a += (s[i]=='A');
         b += (s[i]=='B');
         c += (s[i]=='C');
     }
     cout<<(b==(a+c)? "YES" : "NO")<<endl;
    }
  

    return 0;
}
