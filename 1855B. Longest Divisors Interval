#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
 
int main() {
    int t;
    cin>>t;
    while(t--){
        ll n;
        cin>>n;
       ll count=0;
       for(ll i=1;n%i==0 && i<=n;i++){
           count++;
           
       }
       cout<<count<<endl;
        
    }
 
    return 0;
}
