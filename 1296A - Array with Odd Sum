#include <bits/stdc++.h>
using namespace std;
int main() {
    int t;
    cin>>t;
    while(t--){
    long long n,sum=0;
    bool odd = false, even = false;
    cin>>n;
    
    for(int i=0;i<n;i++){
        long long x;
        cin>>x;
        sum += x;
        if (x % 2 != 0) {
        odd = true;
        } else {
        even = true;
}
        
    }
       if (sum % 2 != 0 || (odd && even)) cout << "YES" << endl;
    else cout<<"NO"<<endl;
 
    }
      
    return 0;
}
