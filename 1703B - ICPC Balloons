#include<bits/stdc++.h>
using namespace std;
 
int main(){
    int n;
    cin>>n;
    while(n--){
        int t;
        char s;
        cin>>t;
        set<char>st;
        int c=0;
        for(int i=0;i<t;i++){
            cin>>s;
           st.insert(s);
            
        }
        cout<<t+st.size()<<endl;
    }
}
