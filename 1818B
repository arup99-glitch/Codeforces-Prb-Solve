#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        if(n==1){
                cout<<"1"<<endl;
        continue;
        }
        if(n%2==1){
                cout<<"-1"<<endl;
        continue;
        }
        else{
            for(int i=2;i<=n;i+=2)
                cout<<i<<" "<<i-1<<endl;
        }
 
    }
}
